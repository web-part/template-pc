{
    "install": true,
    "packages": [
        "defaults",
        "app-module",
        "array",
        "date",
        "emitter",
        "escape",
        "fn",
        "hash",
        "id-maker",
        "json",
        "math",
        "module-manager",
        "object",
        "query",
        "string",
        "style",
        "tasker",
        "timer",
        "tree",
        "api",
        "app",
        "html-parser",
        "local-storage",
        "navigator",
        "package",
        "proxy",
        "script",
        "session-storage",
        "template",
        "url",
        "alert",
        "confirm",
        "dialog",
        "loading",
        "masker",
        "panel",
        "tabs",
        "toast",
        "view"
    ],
    "globalExports": {
        "name": "definejs",
        "allowRequirePackage": true,
        "bind": {
            "alert": "Alert.show",
            "confirm": "Confirm.show",
            "config": "Defaults.config",
            "data": "AppModule.data",
            "launch": "App.launch",
            "load": "Package.load",
            "module": "AppModule.define",
            "panel": "Panel.define",
            "proxy": "Proxy.response",
            "route": "App.route",
            "view": "View.define"
        }
    },
    "thirds": {
        "jquery": "jQuery",
        "font-awesome": true
    },
    "tempDir": "./temp/",
    "outputDir": "./output/",
    "scope": "@definejs/",
    "moduleManager": "module-manager"
}